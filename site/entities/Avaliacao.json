{
  "name": "Avaliacao",
  "type": "object",
  "properties": {
    "cotacaoId": {
      "type": "string",
      "description": "ID da cota\u00e7\u00e3o avaliada"
    },
    "clienteId": {
      "type": "string",
      "description": "ID do cliente que avaliou"
    },
    "transportadoraId": {
      "type": "string",
      "description": "ID da transportadora avaliada"
    },
    "nota": {
      "type": "integer",
      "minimum": 1,
      "maximum": 5,
      "description": "Nota de 1 a 5 estrelas"
    },
    "comentario": {
      "type": "string",
      "description": "Coment\u00e1rio opcional sobre o servi\u00e7o"
    },
    "dataAvaliacao": {
      "type": "string",
      "format": "date-time",
      "description": "Data e hora da avalia\u00e7\u00e3o"
    }
  },
  "required": [
    "cotacaoId",
    "clienteId",
    "transportadoraId",
    "nota"
  ]
}
